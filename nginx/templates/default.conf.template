server {
    listen 80;
    server_name ${NGINX_HOST};

    location /.well-known/acme-challenge/ {
        root /var/www/certbot;
    }

    include /etc/nginx/conf.d/iot_proxy.config;
}

server {
    listen 80;
    server_name flow.${NGINX_HOST};

    include /etc/nginx/conf.d/flow_proxy.config;
}